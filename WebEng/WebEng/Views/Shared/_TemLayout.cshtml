<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Học English Online</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/plugins/fontawesome-free/css/all.min.css")">
    @*<link rel="stylesheet" href="@Url.Content("~/Content/font-awesome-4.7.0/css/font-awesome.min.css")">*@
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css")">
    <!-- iCheck -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/plugins/icheck-bootstrap/icheck-bootstrap.min.css")">
    <!-- JQVMap -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/plugins/jqvmap/jqvmap.min.css")">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/dist/css/adminlte.min.css")">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/plugins/overlayScrollbars/css/OverlayScrollbars.min.css")">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/plugins/daterangepicker/daterangepicker.css")">
    <!-- summernote -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Template/plugins/summernote/summernote-bs4.min.css")">
    @RenderSection("css", required: false)
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="@Url.Content("~/Content/Template/dist/img/loader1.gif")" alt="AdminLTELogo" height="80" width="80">
        </div>

        @*@{ Html.RenderAction("MainHeader", "Layout", new { area = "" });}
        @{ Html.RenderAction("MainSidebar", "Layout", new { area = "" });}*@

        
        @{Html.RenderAction("MainHeader", "Home", "");}

        @{Html.RenderAction("MainSidebar", "Home", "");}
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <section class="content-header">
                <div class="container-fluid">
                    <h4>@RenderSection("header", required: false)</h4>
                </div><!-- /.container-fluid -->
            </section>
            @RenderBody()
        </div>

        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="@Url.Content("~/Content/Template/plugins/jquery/jquery.min.js")"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="@Url.Content("~/Content/Template/plugins/jquery-ui/jquery-ui.min.js")"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="@Url.Content("~/Content/Template/plugins/bootstrap/js/bootstrap.bundle.min.js")"></script>
    <!-- ChartJS -->
    <script src="@Url.Content("~/Content/Template/plugins/chart.js/Chart.min.js")"></script>
    <!-- Sparkline -->
    <script src="@Url.Content("~/Content/Template/plugins/sparklines/sparkline.js")"></script>
    <!-- JQVMap -->
    <script src="@Url.Content("~/Content/Template/plugins/jqvmap/jquery.vmap.min.js")"></script>
    <script src="@Url.Content("~/Content/Template/plugins/jqvmap/maps/jquery.vmap.usa.js")"></script>
    <!-- jQuery Knob Chart -->
    <script src="@Url.Content("~/Content/Template/plugins/jquery-knob/jquery.knob.min.js")"></script>
    <!-- daterangepicker -->
    <script src="@Url.Content("~/Content/Template/plugins/moment/moment.min.js")"></script>
    <script src="@Url.Content("~/Content/Template/plugins/daterangepicker/daterangepicker.js")"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="@Url.Content("~/Content/Template/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js")"></script>
    <!-- Summernote -->
    <script src="@Url.Content("~/Content/Template/plugins/summernote/summernote-bs4.min.js")"></script>
    <!-- overlayScrollbars -->
    <script src="@Url.Content("~/Content/Template/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js")"></script>
    <!-- AdminLTE App -->
    <script src="@Url.Content("~/Content/Template/dist/js/adminlte.js")"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="@Url.Content("~/Content/Template/dist/js/demo.js")"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="@Url.Content("~/Content/Template/dist/js/pages/dashboard.js")"></script>
    @RenderSection("masterjs", required: false)


</body>
</html>
