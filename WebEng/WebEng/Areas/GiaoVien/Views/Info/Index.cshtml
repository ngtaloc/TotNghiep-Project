@model Models.Framework.Giangvien
@{
    ViewBag.Title = "Thông tin cá nhân";
    Layout = "~/Views/Shared/_TemLayout.cshtml";
}

<style>
    /* HIDE RADIO */
    [type=radio] {
        position: absolute;
        opacity: 0;
        width: 0;
        height: 0;
    }

        /* IMAGE STYLES */
        [type=radio] + img {
            cursor: pointer;
        }

        /* CHECKED STYLES */
        [type=radio]:checked + img {
            outline: 2px solid #f00;
        }
</style>
<!-- Content Header (Page header) -->
<!-- Main content -->

@section header{
    @if (TempData["testmsg"] != null)
    {

        <div class="alert alert-info">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            <h5><i class="icon fas fa-exclamation-triangle"></i> Thông báo!</h5>
            @TempData["testmsg"]
        </div>



    }
}

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <!-- Profile Image -->
                <div class="card card-primary card-outline">
                    <div class="card-body box-profile">
                        <div class="text-center">
                            @if (!string.IsNullOrEmpty(Model.TaiKhoan.hinh))
                            {
                                <img class="profile-user-img img-fluid img-circle"
                                     src="~/@Model.TaiKhoan.hinh"
                                     alt="User profile picture">
                            }
                            else
                            {
                                <img class="profile-user-img img-fluid img-circle"
                                     src="https://i.imgur.com/q52cLwE.png"
                                     alt="User profile picture">
                            }

                        </div>
                        <h3 class="profile-username text-center">@Model.TaiKhoan.hovaten</h3>
                        <p class="text-muted text-center">Giáo viên anh văn</p>
                        <ul class="list-group list-group-unbordered mb-3">
                            <li class="list-group-item">
                                <b>Giới tính</b> <a class="float-right">@Model.gioitinh</a>
                            </li>
                            <li class="list-group-item">
                                <b>Ngày sinh</b> <a class="float-right">@Model.ngaysinh.ToString().Split(' ')[0]</a>
                            </li>
                            <li class="list-group-item">
                                <b>Email</b> <a class="float-right">@Model.email</a>
                            </li>
                            <li class="list-group-item">
                                <b>Số điện thoại</b> <a class="float-right">@Model.sdt</a>
                            </li>
                        </ul>

                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
                <!-- About Me Box -->
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">Thông tin</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <strong><i class="fas fa-book mr-1"></i> Giới thiệu</strong>
                        <p class="text-muted">
                            @Model.gioithieu
                        </p>
                        <hr>
                        <strong><i class="fas fa-map-marker-alt mr-1"></i> Địa chỉ</strong>
                        <p class="text-muted">@Model.diachi</p>
                        <hr>


                        <strong><i class="fas fa-pencil-alt mr-1"></i> Skills</strong>
                        <p class="text-muted">
                            @foreach (var item in Model.KyNangGiangViens)
                            {
                            <p class="tag tag-info">@item.KyNang.tenKyNang : @item.CapDo.tenCapDo</p>
                        }


                            </p>

                        </div>
                        <!-- /.card-body -->
                    </div>
                    <!-- /.card -->
                </div>
                <!-- /.col -->
                <div class="col-md-9">
                    <div class="card">
                        <div class="card-header p-2">
                            <ul class="nav nav-pills">
                                <li class="nav-item"><a class="nav-link active" href="#vitien" data-toggle="tab">Ví tiền</a></li>
                                <li class="nav-item"><a class="nav-link" href="#taikhoan" data-toggle="tab">Tài khoản</a></li>
                                <li class="nav-item"><a class="nav-link" href="#settings" data-toggle="tab">Settings</a></li>
                            </ul>
                        </div><!-- /.card-header -->
                        <div class="card-body">
                            <div class="tab-content">
                                <div class="active tab-pane" id="vitien">
                                    <div class="row" style="margin-left:0;">
                                        <div class="alert alert-info alert-dismissible row col-md-9">
                                            Số dư trong ví:<h4 style="padding-left:1em; margin-bottom: 0;"><i class="icon fas fa-info"></i> @String.Format("{0:#,##0}", Model.TaiKhoan.ViTiens.FirstOrDefault().SoDu) đ</h4>

                                        </div>
                                        <button data-toggle="modal" data-target="#modal-Nap" type="button" class="btn btn-success col-md-2" style="margin-bottom:1em; margin-left:1em;"><h5 style="margin-bottom: 0;"><i class="fas fa-hand-holding-usd"></i> Nạp Tiền</h5> </button>
                                    </div>

                                    <p>Lịch sử giao dịch</p>
                                    <div class="timeline timeline-inverse">
                                        @{
                                            string date = null;
                                            foreach (var item in Model.TaiKhoan.ViTiens.FirstOrDefault().LichSuGDs.OrderByDescending(x => x.ThoiGiangGD))
                                            {
                                                if (date != item.ThoiGiangGD.ToString().Split(' ')[0])
                                                {
                                                    date = item.ThoiGiangGD.ToString().Split(' ')[0];
                                                    <div class="time-label">
                                                        <span class="bg-info bg-lime color-palette">
                                                            @item.ThoiGiangGD.ToString().Split(' ')[0]
                                                        </span>
                                                    </div>
                                                }
                                                if (item.LoaiGD == 0)
                                                {
                                                    <div>
                                                        <i class="fas fa-dollar-sign bg-success"></i>
                                                        <div class="timeline-item">
                                                            <span class="time"><i class="far fa-clock"></i> @item.ThoiGiangGD.ToString().Split(' ')[1]</span>
                                                            <h3 class="timeline-header border-0">
                                                                <a href="#">Nạp vào</a> @item.TenGD
                                                            </h3>

                                                        </div>
                                                    </div>

                                                }
                                                else
                                                {
                                                    <div>
                                                        <i class="far fa-plus-square bg-warning"></i>
                                                        <div class="timeline-item">
                                                            <span class="time"><i class="far fa-clock"></i> @item.ThoiGiangGD.ToString().Split(' ')[1]</span>
                                                            <h3 class="timeline-header border-0">
                                                                <a href="#">Mở lớp</a> @item.TenGD
                                                            </h3>
                                                        </div>
                                                    </div>
                                                }

                                            }
                                        }


                                        <div>
                                            <i class="far fa-clock bg-gray"></i>
                                        </div>
                                    </div>
                                    <!-- /.post -->
                                    <!-- /.Modal Nạp Tiền -->
                                    <div class="modal fade" id="modal-Nap">
                                        <div class="modal-dialog ">
